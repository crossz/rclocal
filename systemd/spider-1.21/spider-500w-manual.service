[Unit]
Description=spider-robot live.500.com for manual
After=network.target
After=syslog.target

[Install]
WantedBy=multi-user.target

[Service]
User=root
Group=caiex
#Type=forking

# Start main service
ExecStart=/opt/rclocal/spider-500w-manual.service.sh


# Give up if ping don't get an answer
TimeoutSec=600

Restart=always
PrivateTmp=false
